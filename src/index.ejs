<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>兼容ie8通用脚手架</title>
    <meta name=apple-mobile-web-app-capable content=yes>
    <meta name=apple-mobile-web-app-status-bar-style content=black>
    <meta name=format-detection content="telephone=no">
    <meta name=applicable-device content=mobile>
    <meta name="viewport" content="width=device-width,minimum-scale=1,maximum-scale=1,user-scalable=no"/>
    <script type="text/javascript">
        function setFontSize() {
            var deviceWidth = document.documentElement.clientWidth;
            document.documentElement.style.fontSize = deviceWidth / 3.75 + "px";
        }
        window.onresize = setFontSize;
        setFontSize();
    </script>
    <!--[if (IE 8)|(IE 9)]>
    <script src="//web.ewt360.com/attainment/common/jquery-1.11.2.min.js"></script>
    <script src="//web.ewt360.com/attainment/common/jquery.xdomainrequest.min.js"></script>
    <![endif]-->

    <!--[if lt IE 9]>
    <script src="lib/es5-shim.min-4.5.10.js"></script>
    <script src="lib/es5-sham.min-4.5.10.js"></script>
    <![endif]-->
</head>
<body>
<div id="app"></div>
<% if (htmlWebpackPlugin.options.dllName) { %>
<script src="<%=htmlWebpackPlugin.options.dllName%>"></script>
<% } %>

</body>
</html>
